{
  "manifest_version": 3,
  "name": "VGC Teambuilder Colors",
  "version": "1.0.1",
  "description": "Edits stat bars in teambuilder to reflect levels 5 and 50",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "icon.png",
    "default_title": "VGC Teambuilder Colors"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://play.pokemonshowdown.com/teambuilder"],
      "js": ["content.js"]
    }
  ]
}
